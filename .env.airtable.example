# Example Airtable and Gemini API configuration file
# Copy this file to .env.airtable and fill in your credentials

# Airtable Integration
AIRTABLE_BASE_ID=app123456example
AIRTABLE_TABLE_NAME=FrameData
AIRTABLE_API_KEY=key123456example

# Google Gemini API
GEMINI_API_KEY=your_gemini_api_key_here

# Configuration Options
# Set to true to enable Airtable updates
ENABLE_AIRTABLE_UPDATES=true

# Set to true to use Google Gemini API for enhanced OCR text analysis
USE_GEMINI=true 